{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "dist/jquery.min.js",
        "dist/jquery.js"
      ]
    },
    {
      "library": "jquery-validation@1.19.2",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [
        "dist/jquery.validate.js",
        "dist/jquery.validate.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "dist/jquery.validate.unobtrusive.js",
        "dist/jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "font-awesome@6.4.0",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.min.css",
        "js/all.min.js",
        "webfonts/fa-brands-400.ttf",
        "webfonts/fa-brands-400.woff2",
        "webfonts/fa-regular-400.ttf",
        "webfonts/fa-regular-400.woff2",
        "webfonts/fa-solid-900.ttf",
        "webfonts/fa-solid-900.woff2",
        "webfonts/fa-v4compatibility.ttf",
        "webfonts/fa-v4compatibility.woff2"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "bootstrap@5.3.0",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.min.css",
        "js/bootstrap.bundle.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "animate.css@4.1.1",
      "destination": "wwwroot/lib/animate.css/",
      "files": [
        "animate.min.css"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/"
    }
  ]
}
