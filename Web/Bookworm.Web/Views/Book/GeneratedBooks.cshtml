@model IEnumerable<BookViewModel>

@{
    ViewData["Title"] = "Generated Books";
    var titleCardModel = new TitleCardPartialViewModel
    {
        Title = ViewData["Title"] as string,
        Count = Model.Count()
    };
}

@section Styles {
    <link rel="stylesheet" href="~/css/bookListing.css" asp-append-version="true" />
}

<partial name="PartialViews/_TitleCardPartial" model=titleCardModel />

<partial name="PartialViews/_BookListingPartial" model="@Model" />