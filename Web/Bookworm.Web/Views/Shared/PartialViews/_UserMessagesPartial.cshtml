@if (TempData[MessageConstant.ErrorMessage] != null)
{
    ViewData["Toast-Header-Class"] = "toast-header-error";
    ViewData["Toast-Title"] = "Error Message";
    ViewData["Toast-Message"] = TempData[MessageConstant.ErrorMessage];
    <partial name="PartialViews/_ToastNotificationPartial" />
    <script src="~/js/toast.js"></script>
}
