@if (TempData[TempDataMessageConstant.ErrorMessage] != null)
{
    this.ViewData["Toast-Header-Class"] = "toast-header-error";
    this.ViewData["Toast-Title"] = "Error Message";
    this.ViewData["Toast-Message"] = TempData[TempDataMessageConstant.ErrorMessage];
    <partial name="PartialViews/_ToastNotificationPartial" />
    <script src="~/js/toast.js"></script>
}
