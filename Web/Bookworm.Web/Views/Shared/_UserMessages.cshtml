@if (TempData[MessageConstant.ErrorMessage] != null)
{
    <script>
        $(function () {
            toastr.error('@TempData[MessageConstant.ErrorMessage].ToString()');
        })
    </script>
}
@if (TempData[MessageConstant.WarningMessage] != null)
{
    <script>
        $(function () {
            toastr.warning('@TempData[MessageConstant.WarningMessage].ToString()');
        })
    </script>
}
@if (TempData[MessageConstant.SuccessMessage] != null)
{
    <script>
        $(function () {
            toastr.options.closeButton = true;
            toastr.options.timeOut = 10000;
            toastr.success('When admin approves your quote others may see it', '@TempData[MessageConstant.SuccessMessage].ToString()');
        })
    </script>
}
